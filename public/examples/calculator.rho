new calculatorAdd, calculatorSub, calculatorMul, calculatorDiv in {
  for ( @a, @b <= calculatorAdd) {
    stdout!(a + b)
  } |
  for ( @a, @b <= calculatorSub) {
    stdout!(a - b)
  } |
  for ( @a, @b <= calculatorMul) {
    stdout!(a * b)
  } |
  for ( @a, @b <= calculatorDiv) {
    stdout!(a / b)
  } |
  calculatorAdd!(2, 3) |
  calculatorSub!(2, 9) |
  calculatorMul!(4, 5) |
  calculatorDiv!(8, 2)
}
